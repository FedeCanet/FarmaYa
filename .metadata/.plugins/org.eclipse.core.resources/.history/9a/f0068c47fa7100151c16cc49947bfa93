package com.is3;

import java.io.IOException;

import javax.servlet.RequestDispatcher;
import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

public class FarmaciaServlet extends HttpServlet {

	@Override
	protected void doGet(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException {
		// TODO Auto-generated method stub
		super.doGet(request, response);
	}

	@Override
	protected void doPost(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException {
		// TODO Auto-generated method stub
				
		String farmacia = request.getParameter("idFarmacia");
		String farmaciaPresentacion = Parameters.getParameter("resultadodebusqueda");
		String farmaciaProductos = Parameters.getParameter("resultadodebusqueda");
		String farmaciaCarrito = Parameters.getParameter("resultadodebusqueda");
		request.setAttribute("farmaciaPresentacion", htmlBusqueda);
		request.setAttribute("farmaciaProductos", htmlBusqueda);
		request.setAttribute("farmaciaCarrito", htmlBusqueda);
		RequestDispatcher rd = request.getRequestDispatcher("/farmacia.jsp");
        rd.forward(request, response);
	}	
}


